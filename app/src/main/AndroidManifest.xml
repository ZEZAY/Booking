<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.iproz.mycreateapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_penguin_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_penguin_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
        </activity>
        <activity android:name=".ui.CreateGroup">
        </activity>
        <activity
                android:name=".ui.LoginActivity"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.RegisterActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".ui.Join">
        </activity>
        <activity android:name=".ui.Booking">
        </activity>
        <activity android:name=".ui.CalendarActivity">
        </activity>
        <activity android:name=".ui.CheckLogout">
        </activity>
    </application>

</manifest>